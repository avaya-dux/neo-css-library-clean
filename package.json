{
  "name": "css-library",
  "author": "Mohammad Razian",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --config ./jest.config.json --watchAll=true",
    "build": "parcel build neo/neo-scss/neo.scss",
    "build:to": "parcel build neo/neo-scss/neo.scss --dist-dir",
    "build:for-publish": "npm run build:package && npm run build:package-old",
    "build:package": "parcel build neo/neo-scss/neo.scss --dist-dir neo-npm-package",
    "build:package-old": "parcel build neo/neo-scss/neo.scss --dist-dir neo-npm-package/neo/dist/css/neo",
    "watch": "parcel watch neo/neo-scss/neo.scss",
    "pull-icons": "cd figma-API && node -r esm design-system-functions.js && node -r esm ./icons-functions/icon-css-generator.js"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.6.0",
    "axios": "^0.21.1",
    "cypress": "^4.12.1",
    "cypress-image-snapshot": "^4.0.1",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "figma-transformer": "^2.1.0",
    "jest": "^27.4.5",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "parcel": "^2.6.0",
    "postcss": "^8.4.14",
    "prettier": "^2.3.0",
    "replace-in-file": "^6.2.0",
    "style-dictionary": "^2.10.3",
    "susy": "^3.0.7",
    "webfonts-generator": "^0.4.0"
  }
}
