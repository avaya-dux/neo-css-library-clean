@font-face {
font-family: "{{ fontName }}";
src: {{{ src }}};
}

[class^="{{classPrefix}}"]:before, [class*="{{classPrefix}}"]:before {
font-family: {{ fontName }} !important;
-webkit-font-feature-settings: "liga";
-moz-font-feature-settings: "liga=1";
-moz-font-feature-settings: "liga";
-ms-font-feature-settings: "liga" 1;
font-feature-settings: "liga";
-webkit-font-variant-ligatures: discretionary-ligatures;
font-variant-ligatures: discretionary-ligatures;
font-style: normal;
font-weight: normal;
font-variant: normal;
text-transform: none;
line-height: 1;
-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
display: inline-block;
height: auto;
width: auto;
letter-spacing: 0;
min-height: 0;
speak: none;
text-align: center;
text-transform: none;
vertical-align: middle;
font-size: 32px;
}

[dir="rtl"][class^="{{classPrefix}}"]:before, [dir="rtl"][class^="{{classPrefix}}"]:before
{
transform: scaleX(-1) !important;
}

{{# each codepoints }}
.{{ ../classPrefix }}{{ @key }}:before, .aoc-icon-{{ @key}}:before {
content: "\\{{ this }}";
}

{{#ifEquals @key "address-book"}}
.{{ ../classPrefix }}-address-book-inverted:before, .aoc-icon-address-book-inverted:before,
.{{ ../classPrefix }}-address-folder:before, .aoc-icon-address-folder:before {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "agents"}}
.{{ ../classPrefix }}-agents-inverted:before, .aoc-icon-agents-inverted:before,
.{{ ../classPrefix }}-my-agents:before, .aoc-icon-my-agents:before, .{{ ../classPrefix }}-customer-consult:before,
.aoc-icon-customer-consult:before {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "arrow-down"}}
.{{ ../classPrefix }}-inbound:before, .aoc-icon-inbound:before {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "arrow-up"}}
.{{ ../classPrefix }}-outbound:before, .aoc-icon-outbound:before, .{{ ../classPrefix }}-outbound-channel:before,
.aoc-icon-outbound-channel:before {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "awfos"}}
.{{ ../classPrefix }}-awfos-portal:before, .aoc-icon-awfos-portal:before, .{{ ../classPrefix }}-rtl-awfos:before,
.aoc-icon-rtl-awfos:before, .{{ ../classPrefix }}-rtl-evaluation:before, .aoc-icon-rtl-evaluation:before, {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "bot-avatar"}}
.{{ ../classPrefix }}-robot:before, .aoc-icon-robot:before {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "bullet-list"}}
.{{ ../classPrefix }}-list:before, .aoc-icon-list:before, .{{ ../classPrefix }}-list-closed:before,
.aoc-icon-list-closed:before, .{{ ../classPrefix }}-list-clock:before, .aoc-icon-list-clock:before,
.{{ ../classPrefix }}-list-file:before, .aoc-icon-list-file:before,{
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "chat"}}
.{{ ../classPrefix }}-chat-channel:before, .aoc-icon-chat-channel:before, .{{ ../classPrefix }}-messaging:before,
.aoc-icon-messaging:before, {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{#ifEquals @key "chat-alerting"}}
.{{ ../classPrefix }}-messaging-alerting:before, .aoc-icon-messaging-alerting:before, {
content: "\\{{ this }}";
}
{{/ifEquals}}

{{/ each }}