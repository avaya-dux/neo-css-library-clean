{
	"name": "staging",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "astro preview --host",
		"build": "astro build",
		"dev": "astro dev",
		"format": "biome format --write ./src",
		"lint": "biome ci ./src",
		"style-check": "biome check --write ./src",
		"clean": "rm -rf coverage dist",
		"github": "yarn && yarn clean && yarn lint && yarn build",
		"all": "yarn style-check && yarn build"
	},
	"devDependencies": {
		"@astrojs/react": "3.6.2",
		"@astrojs/svelte": "5.7.1",
		"@avaya/neo-icons": "1.8.16",
		"@biomejs/biome": "1.9.2",
		"@types/react": "18.3.10",
		"@types/react-dom": "18.3.0",
		"astro": "4.15.9",
		"clsx": "2.1.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"svelte": "4.2.19",
		"typescript": "5.6.2"
	}
}
