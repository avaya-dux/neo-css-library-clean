{
  "name": "staging",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "astro preview",
    "test": "cypress run --e2e -b chrome --spec cypress/e2e/pages/*.cy.ts",
    "build": "astro build",
    "dev": "astro dev",
    "cypress": "start-server-and-test 3000",
    "cypress:open": "cypress open --e2e -b chrome",
    "cypress:update": "cypress run --e2e --env updateSnapshots=true -b chrome --spec cypress/e2e/pages/*.cy.ts",
    "lint": "eslint ./src",
    "clean": "rm -rf coverage dist",
    "github": "yarn && yarn clean && yarn lint && yarn build",
    "all": "yarn github && yarn cypress"
  },
  "devDependencies": {
    "@astrojs/react": "1.1.4",
    "@astrojs/svelte": "1.0.1",
    "@astrojs/vue": "1.1.0",
    "@astronomer/eslint-config-astro": "1.0.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "astro": "1.4.2",
    "clsx": "1.2.1",
    "cypress": "^10.9.0",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-real-events": "^1.7.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "start-server-and-test": "^1.14.0",
    "svelte": "3.50.1",
    "typescript": "^4.8.4",
    "vue": "3.2.40"
  }
}
