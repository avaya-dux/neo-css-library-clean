    "jest": "27.5.1",
    "jest-fetch-mock": "3.0.3",
    "lodash": "4.17.21",
    "node-fetch": "2.6.7",
    "parcel": "2.7.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "replace-in-file": "6.3.5",
